 this.playerHealth += 10;\n      } else {\n        this.playerHealth = 100;\n      }\n\n      this.monsterAttacks();\n    },\n    giveUp: function giveUp() {\n      this.gameIsRunning = false;\n    },\n    monsterAttacks: function monsterAttacks() {\n      var damage = this.calculateDamage(5, 12);\n      this.playerHealth -= damage;\n      this.checkWin();\n      this.turns.unshift({\n        isPlayer: false,\n        text: 'Monster hits Player for' + damage\n      });\n    },\n    calculateDamage: function calculateDamage(min, max) {\n      return Math.max(Math.floor(Math.random() * max) + 1, min);\n    },\n    checkWin: function checkWin() {\n      if (this.monsterHealth <= 0) {\n        if (confirm('You won! New Game?')) {\n          this.startGame();\n        } else {\n          this.gameIsRunning = false;\n        }\n\n        return true;\n      } else if (this.playerHealth <= 0) {\n        if (confirm('You lost! New Game?')) {\n          this.startGame();\n        } else {\n          this.gameIsRunning = false;\n        }\n\n        return true;\n      }\n\n      return false;\n    }\n  }\n};",{"version":3,"sources":["Game.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,KAAA,EAAA,EAFA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,YAAA,EAAA,GADA;AAEA,MAAA,aAAA,EAAA,GAFA;AAGA,MAAA,aAAA,EAAA,KAHA;AAIA,MAAA,KAAA,EAAA;AAJA,KAAA;AAMA,GAZA;AAaA,EAAA,OAAA,EAAA;AACA,IAAA,SAAA,EAAA,qBAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,GAAA;AACA,WAAA,aAAA,GAAA,GAAA;AACA,KALA;AAMA,IAAA,MAAA,EAAA,kBAAA;AACA,UAAA,MAAA,GAAA,KAAA,eAAA,CAAA,CAAA,EAAA,EAAA,CAAA;AACA,WAAA,aAAA,IAAA,MAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA;AACA,QAAA,QAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,4BAAA;AAFA,OAAA;;AAIA,UAAA,KAAA,QAAA,EAAA,EAAA;AACA;AACA;;AACA,WAAA,cAAA;AACA,KAjBA;AAkBA,IAAA,aAAA,EAAA,yBAAA;AACA,WAAA,aAAA,IAAA,KAAA,eAAA,CAAA,EAAA,EAAA,EAAA,CAAA;;AACA,UAAA,KAAA,QAAA,EAAA,EAAA;AACA;AACA;;AACA,WAAA,cAAA;AACA,KAxBA;AAyBA,IAAA,IAAA,E